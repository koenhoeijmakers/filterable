{
    "name": "koenhoeijmakers/laravel-filterable",
    "description": "A Laravel trait that handles filtering through request variables.",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.5.*|5.6.*|5.7.*"
    },
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Koen Hoeijmakers",
            "email": "koen@hoeijmakers.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "KoenHoeijmakers\\LaravelFilterable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KoenHoeijmakers\\LaravelFilterable\\FilterableServiceProvider"
            ]
        }
    }
}
